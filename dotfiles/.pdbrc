alias sh from ptpython.repl import embed, run_config; embed(history_filename="/tmp/pdb_history", configure=run_config, locals=locals(), globals=globals())

alias dpp __import__("devtools").debug(%1)

alias p from pathlib import Path;from rich.console import Console;from rich.theme import Theme;Console(theme=Theme.read(Path("~/.config/rich/rich.ini").expanduser().as_posix())).print(%1)
alias pp from pathlib import Path;from rich.console import Console;from rich.theme import Theme;Console(theme=Theme.read(Path("~/.config/rich/rich.ini").expanduser().as_posix())).print(%1)
alias pi from pathlib import Path;from rich.console import Console;from rich.theme import Theme;Console(theme=Theme.read(Path("~/.config/rich/rich.ini").expanduser().as_posix())).inspect(%1)

alias sqlfmt print(__import__("sqlparse").format(str(%1), reindent=True, keyword_case="upper"))

alias t type(%1)

alias osexit __import__("os")._exit(0)

alias timeit __import__("fohn").timeit(%1)
